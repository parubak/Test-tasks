CREATE SCHEMA `db_conference` ;
CREATE TABLE `db_conference`.`conferences` (
                                               `id` INT NOT NULL AUTO_INCREMENT,
                                               `title` VARCHAR(255) NOT NULL,
                                               `data` DATE NOT NULL,
                                               `map_lat` DOUBLE NULL,
                                               `map_lng` DOUBLE NULL,
                                               `country` VARCHAR(100) NOT NULL,
                                               PRIMARY KEY (`id`));